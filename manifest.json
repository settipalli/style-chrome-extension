{
  "manifest_version": 2,
  "name": "Style",
  "version": "0.9.0",
  "author": "Santhoshkumar Settipalli",
  "description": "Set custom style for the elements on the current webpage.",

  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],

  "browser_action": {},

  "background": {
    "scripts": ["background.js"]
  },

  "web_accessible_resources": ["styles.css"]
}